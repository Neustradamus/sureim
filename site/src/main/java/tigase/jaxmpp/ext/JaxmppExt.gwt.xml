<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="JaxmppExt">
  <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="tigase.jaxmpp.gwt.JaxmppGWT"/>
    
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name="com.google.gwt.user.theme.standard.Standard"/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <!-- Other module inherits                                      -->
  <!-- <inherits name="xtigase.http.xtigase_common"/> -->
  
  <!-- Specify the app entry point class.                         -->
<!--    <entry-point class="eu.hilow.xode.client.EntryPoint"/>-->

<!--    <replace-with class="eu.hilow.xode.client.ClientFactoryImpl">
        <when-type-is class="eu.hilow.xode.client.ClientFactory"/>
    </replace-with>-->


    <extend-property name="locale" values="pl"/>

<!--    <set-property name="gwt.logging.enabled" value="FALSE"/> -->

<!--    <set-property name="user.agent" value="safari"/>-->

    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <set-property name="gwt.logging.logLevel" value="ALL"/>
    <set-property name="gwt.logging.consoleHandler" value="ENABLED" />
    <set-property name="gwt.logging.popupHandler" value="DISABLED" />

    <set-property name="compiler.stackMode" value="emulated"/>
    <set-configuration-property name="compiler.emulatedStack.recordFileNames" value="true"/>
    
<!--
  <set-property name='gwt.logging.enabled' value='ENABLED' />
  <set-property name='gwt.logging.logLevel' value='ALL' />
  <set-property name='gwt.logging.popupHandler' value='DISABLED'/>
  <set-property name='gwt.logging.consoleHandler' value='ENABLED' />
  <set-property name='gwt.logging.developmentModeHandler' value='DISABLED' />
  <set-property name="gwt.logging.firebugHandler" value="DISABLED" />
  <set-property name="gwt.logging.systemHandler" value="DISABLED" />
  -->
  <!-- Specify the paths for translatable code                    -->
    <source path="client"/>
</module>
